{
  "name": "saitama-premium",
  "version": "0.1.0",
  "bin": {
    "saitama-premium": "bin/saitama-premium.js"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode local",
    "serve-dev": "vue-cli-service serve --mode dev",
    "serve-prd": "vue-cli-service serve --mode prd",
    "build": "vue-cli-service build --mode local",
    "build-dev": "vue-cli-service build --mode dev",
    "build-prd": "vue-cli-service build --mode prd",
    "lint": "vue-cli-service lint",
    "get-merchant": "run-s python:venv python:activate python:pip python:get-merchant",
    "create-summary": "run-s python:venv python:activate python:pip python:create-summary",
    "deploy-dev": "run-s lint build-dev deploy:dev",
    "deploy-prd": "run-s lint build-prd deploy:prd",
    "python:venv": "python3 -m venv .env",
    "python:activate": "source .env/bin/activate",
    "python:pip": "pip install -r requirements.txt",
    "python:get-merchant": "python3 script/get_merchant.py",
    "python:create-summary": "python3 script/create_summary.py",
    "deploy:dev": "cdk deploy --require-approval never",
    "deploy:prd": "cdk deploy --require-approval never"
  },
  "dependencies": {
    "constructs": "^10.0.127",
    "core-js": "^3.8.3",
    "gmap-vue": "^3.5.2",
    "source-map-support": "^0.5.21",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.100.0",
    "@aws-cdk/aws-cloudfront": "^1.100.0",
    "@aws-cdk/aws-iam": "^1.100.0",
    "@aws-cdk/aws-route53": "^1.100.0",
    "@aws-cdk/aws-route53-targets": "^1.100.0",
    "@aws-cdk/aws-s3": "^1.100.0",
    "@aws-cdk/aws-s3-deployment": "^1.100.0",
    "@aws-cdk/core": "^1.100.0",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@mdi/js": "^6.6.96",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "aws-cdk": "^1.100.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
