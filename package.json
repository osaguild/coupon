{
  "name": "saitama-premium",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "get-merchant": "run-s python:venv python:activate python:pip python:get-merchant",
    "create-summary": "run-s python:venv python:activate python:pip python:create-summary",
    "deploy-dev": "run-s lint build python:venv python:activate python:pip deploy:dev",
    "deploy-prd": "run-s lint build python:venv python:activate python:pip deploy:prd",
    "python:venv": "python3 -m venv .env",
    "python:activate": "source .env/bin/activate",
    "python:pip": "pip install -r requirements.txt",
    "python:get-merchant": "python3 script/get_merchant.py",
    "python:create-summary": "python3 script/create_summary.py",
    "deploy:dev": "cdk deploy saitama-premium-dev --require-approval never",
    "deploy:prd": "cdk deploy saitama-premium-prd --require-approval never"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "gmap-vue": "^3.5.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
